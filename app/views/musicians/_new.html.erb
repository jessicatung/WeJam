<div data-reveal=" ">
  <h2 class="sign-up">Play with someone</h2>
  <!-- <div class="sign_up_confirmation"></div> -->
  <!--     <div class="errors">

  </div> -->

  <div class="row">
    <div class="small-5 columns">
      <%= form_for @musician do |f|%>
      <p>
        <%= f.text_field :username, :placeholder =>"Musician Name" %>
      </p>
      <p>
        <%= f.text_field :email, :placeholder =>"Email"  %>
      </p>
      <p>
      <!-- Password<br> -->
        <%= f.password_field :password %>
      </p>
      <p>
        <%= f.text_field :location, :placeholder =>"Location"  %>
      </p>
      <p>
        <%= f.text_field :instrument, :placeholder =>"Instrument"  %>
      </p>
    </div>
    <div class="small-5 columns">
      <p>
        <%= f.text_field :genre, :placeholder =>"Genre"  %>
      </p>
      <p>
        <%= f.text_field :skill_level, :placeholder =>"Skill Level"  %>
      </p>
      <p>
        <%= f.text_field :url, :placeholder =>"Link Your Sound Clips" %>
      </p>
      <p>
        <%= f.text_field :gravatar_url, :placeholder =>"Provide a   Profile Picture Link"  %>
      </p>
      <p>
        <%= f.text_area :notes, :placeholder =>"Describe Your Jam Style"  %>
      </p>
    </div>
  </div>
  <div class="row">
    <div class="small-4  small-offset-4 end columns">
      <p>
        <%= submit_tag "Jam With Us!",  :class=>"button postfix" %>
      </p>
    </div>
  </div>
    <% end %>
    <a class="closing close-reveal-modal">Ã—</a>
</div>

